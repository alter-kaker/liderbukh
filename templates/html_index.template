  <title>@!meta['header_text']!@</title>
  <link rel="canonical" href="https://liderbukh.jews.international/">
  <style>
  body {
    width: 85%;
    margin: auto;
    }
  footer li {
	list-style: none;
	display: block;
	margin: 1em;
	text-align: center;
    }
  footer {
	border-top: solid 1px black;
  }
  </style>
  </head>
  <body>
    <header>
      <h1>@!meta['header_text']!@</h1>
    </header>
    <main>
        <ul>
          <!--(for category in index_data)-->
          <li><h2>@!category['meta']['category_name']!@</h2></li>
            <ul>
              <!--(for song in category['songs'])-->
