 <!DOCTYPE=html>
<html lang="yi" dir='rtl'>
    <head>
        <meta charset="utf-8"> 
        <meta name="viewport" content="width=device-width, initial-scale=1">
        <title>@!name!@</title>
        <link rel="canonical" href="@!canonical_url!@">
        <link rel="stylesheet" type="text/css" href="style.css" media="screen"/>
    </head>
    <body>
        <header>
            <h1>@!name!@</h1>
            <nav>
                <ul>
                    <!--(for category in tree.children)-->
                    <li>@!category.meta['name']!@</li>
                    <!--(end)-->
                </ul>
            </nav>
        </header>
        
        <main>
            <!--(for child in children)-->
            <article>
                @!child.data['notes']!@
                <img src="@!child.formats['image'].link!@" />
                <a href="@!child.formats['leadsheet'].link!@">pdf</a> <a href="@!child.formats['music'].link!@">ly</a>
            </article>
            <!--(end)-->
        </main>
        <footer>
        <ul>
        <!--(for key in tree.meta['footer_links'].keys() )-->
            <li><a href="@!tree.meta['footer_links'][key]!@">@!key!@</a></li>
        <!--(end)-->
        </ul>
        </footer>
    </body>
</html>
